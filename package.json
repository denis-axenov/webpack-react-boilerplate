{
  "name": "webpack-react-boilerplate",
  "version": "1.0.0",
  "description": "Webpack bundler",
  "main": "src/scripts/main.js",
  "scripts": {
    "dev": "webpack serve --mode=development",
    "dev:lint": "webpack serve --mode=development --env lint",
    "watch": "webpack --mode=development --watch",
    "watch:lint": "webpack --mode=development --watch --env lint",
    "build": "webpack --mode=production",
    "lint": "eslint src/scripts && stylelint src/styles/**/*.scss",
    "lint:fix": "eslint src/scripts --fix && stylelint src/styles/**/*.scss --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --collect-coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denis-axenov/webpack-react-boilerplate.git"
  },
  "keywords": [
    "webpack",
    "bundler",
    "static",
    "scss",
    "eslint",
    "stylelint"
  ],
  "author": "Denis Axenov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/denis-axenov/webpack-react-boilerplate/issues"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@jest/globals": "^29.7.0",
    "@swc/core": "^1.9.3",
    "@swc/jest": "^0.2.37",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^7.1.2",
    "cssnano": "^7.0.6",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-webpack-plugin": "^4.2.0",
    "globals": "^15.12.0",
    "html-webpack-plugin": "^5.6.3",
    "jest": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "postcss": "^8.4.49",
    "postcss-loader": "^8.1.1",
    "postcss-preset-env": "^10.1.1",
    "postcss-scss": "^4.0.9",
    "sass": "^1.81.0",
    "sass-loader": "^16.0.3",
    "scss-toolkit": "^1.0.4",
    "style-loader": "^4.0.0",
    "stylelint": "^16.11.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-scss": "^6.10.0",
    "stylelint-webpack-plugin": "^5.0.1",
    "swc-loader": "^0.2.6",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
